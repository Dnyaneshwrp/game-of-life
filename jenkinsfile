pipeline {
    agent jav8 
    stages {
        stage('source') {
            steps {
                git 'https://github.com/wakaleo/game-of-life.git'
                
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
               # sh 'mkdir warfile'
            }
        
        }
        stage('deploy') {
            steps {
                sh 'mv gameoflife-web/target/*.war  /home/jenkins/warfile'
               
            }
        
        }
        
    }
}
